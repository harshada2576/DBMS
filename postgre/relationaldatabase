--Field/Column Level Constraints or in-line constraint
CREATE TABLE EmpTable1

(

EmpNo INT PRIMARY KEY,

"First Name" VARCHAR(15) NOT NULL,

LastName VARCHAR(15) NOT NULL,

Address VARCHAR(50) UNIQUE NULL,

City VARCHAR(20) DEFAULT 'MUMBAI',

State VARCHAR(20) DEFAULT 'MAHARASHTRA',

Salary FLOAT DEFAULT 100 CHECK (Salary > 0) NULL

);
--Table Level Constraints / out-of-line constraints
CREATE TABLE EmpTable1

(

EmpNo INT,

"First Name" VARCHAR(15) NOT NULL,

LastName VARCHAR(15) NOT NULL,

Address VARCHAR(50) NULL,

City VARCHAR(20) DEFAULT 'MUMBAI',

State VARCHAR(20) DEFAULT 'MAHARASHTRA',

Salary FLOAT DEFAULT 100 NULL,

PRIMARY KEY(EmpNo),

UNIQUE(Address),

CHECK (Salary > 0)

);
CREATE TABLE EmpTable1

(

EmpNo INT,

"First Name" VARCHAR(15) NOT NULL,

LastName VARCHAR(15) NOT NULL,

Address VARCHAR(50) NULL,

City VARCHAR(20) DEFAULT 'MUMBAI',

State VARCHAR(20) DEFAULT 'MAHARASHTRA',

Salary FLOAT DEFAULT 100 NULL,

PRIMARY KEY(EmpNo),

UNIQUE(Address),

CHECK (Salary > 0 AND Salary < 100000)

);
DROP TABLE EmpTable1;

CREATE TABLE EmpTable1

(

EmpNo INT,

"First Name" VARCHAR(15) NOT NULL,

LastName VARCHAR(15) NOT NULL,

Address VARCHAR(50) NULL,

City VARCHAR(20) DEFAULT 'MUMBAI',

State VARCHAR(20) DEFAULT 'MAHARASHTRA',

Salary FLOAT DEFAULT 100 NULL,

PRIMARY KEY(EmpNo),

UNIQUE(Address),

CHECK (Salary BETWEEN 0 AND 100000)

);


CHECK (Salary = 10000 OR Salary = 20000 OR Salary = 30000)

CHECK (Salary IN(10000, 20000, 30000))

CHECK (Salary = ANY('{10000, 20000, 30000}' :: int[]))

CHECK ((Salary > 0 AND Salary <= 10000) OR (Salary >= 20000 AND Salary <= 30000))
--PRIMARY KEY Constraint
INSERT INTO EmpTable1

