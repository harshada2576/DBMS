DROP TABLE EmpTable;
CREATE TABLE EmpTable
(
  EmpNo INT,
  "First Name" VARCHAR(15),  
  LastName VARCHAR(15),
  Address VARCHAR(50),
  City VARCHAR(20), 
  State VARCHAR(20),	
  Salary FLOAT
);
